{
    "name": "era269/micro-object",
    "type": "library",
    "description": "Object <=> Service => MicroService <=> Microobject",
    "keywords": ["OOP","DDD","Event"],
    "license": "MIT",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "era269/normalizable": "^0.5",
        "psr/event-dispatcher": "^1.0"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.85",
        "phpstan/phpstan-phpunit": "^0.12.18",
        "phpunit/phpunit": "^9.5"
    },
    "authors": [
        {
            "name": "Yuriy Antonuyk",
            "email": "era269@ukr.net"
        },
        {
            "name": "Dmitriy Ivanov",
            "email": "dmitry.ivanov.zagranju@gmail.com"
        }
    ],
    "config": {
        "platform": {
            "php": "8.0"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Era269\\Microobject\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Era269\\Microobject\\Tests\\": "tests",
            "Era269\\Microobject\\Example\\": "example"
        }
    }
}

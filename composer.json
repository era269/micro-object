{
    "name": "era269/micro-object",
    "description": "Object <=> Service => MicroService <=> Microobject",
    "type": "library",
    "prefer-stable": true,
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "phpstan/phpstan": "^0.12.57"
    },
    "authors": [
        {
            "name": "Yuriy Antonuyk",
            "email": "yuriy.mihaylovich.antonyuk@gmail.com"
        },
        {
            "name": "Dmitriy Ivanov",
            "email": "dmitry.ivanov.zagranju@gmail.com"
        }
    ],
    "config": {
        "platform": {
            "php": "8.0"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Era269\\Microobject\\": "src",
            "Era269\\Example\\": "example"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Era269\\Microobject\\Tests\\": "tests"
        }
    }
}
